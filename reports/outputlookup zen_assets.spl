| table agent_uuid,aws_ec2_instance_ami_id, aws_ec2_instance_id, aws_vpc_id, created_at,first_scan_time, first_seen, has_agent, has_plugin_results, hostnames, installed_software, ipv4s, last_authenticated_scan_date, last_seen, operating_systems, state, system_types, uuid
| outputlookup output_format=splunk_mv_csv zen_assets.csv